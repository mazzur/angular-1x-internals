rules:
  indent:
    - 2
    - 4
  quotes:
    - 2
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - always
  no-param-reassign:
    - 2
    - props: false
  comma-dangle:
    - 2
    - 'never'
  object-curly-spacing:
    - 0
  max-len:
    - 2
    - 140
  line-break-style:
    - 0
  no-use-before-define:
    - 0
env:
  es6: true
  node: true
  browser: true
  jasmine: true
parserOptions:
  ecmaVersion: 6
  sourceType: 'module'
extends: 'airbnb'
